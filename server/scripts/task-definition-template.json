{
  "family": "extender",
  "containerDefinitions": [
    {
      "image": "%IMAGE%",
      "name": "extender",
      "cpu": 10,
      "memory": 2048,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 9000
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "prod-logs",
          "awslogs-region": "eu-west-1",
          "awslogs-stream-prefix": "extender"
        }
      }
    }
  ]
}
